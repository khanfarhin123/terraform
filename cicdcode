node('main')
{
    stage('checkout'){
        checkout scmGit(branches: [[name: 'main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/khanfarhin123/terraform.git']])
    }
    stage('teraform build'){
        sh  "terraform init"
        sh  "terraform plan"
        sh  "terraform apply -auto-approve"  
        
    }
}
